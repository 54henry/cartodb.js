{
  "source": {
    "include": [
      "src"
    ]
  },
  "templates": {
    "default": {
      "outputSourceFiles": false,
      "useLongnameInNav":  true
    }
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "recurse": true,
    "destination": "./docs/v4-internal",
    "template": "./jsdoc-template"
  }
}
